<div class="opie-form-top-background" style="padding-top:66px;">
</div>
<div class="col-xs-1 col-sm-2 col-md-3 col-lg-3 opie-form-top-background" style="padding-bottom: 85px;"></div>
<div class="col-xs-10 col-sm-8 col-md-6 col-lg-6 card-form">
  <div class="card card-shadow">
    <div class="header">
      <h2 class="font-weight-300" style="padding-bottom: 20px">Timed 10-Meter Walk Test
        <span class="pull-right">
          <%= image_tag('opie.png', class: 'img-small img-raise', style: 'margin-top: -5px;') %>
        </span>
      </h2>
      <form style="padding-top: 15px;">
        <div class="form-group">
          <h4 class="font-weight-300">Name</h4>
          <input type="text" name="name" id="name" class="form-control form-flatline-240" placeholder="">
        </div>
        <div class="form-group">
          <h4 class="font-weight-300">Date</h4>
          <input type="text" name="date" id="date" class="form-control form-flatline-240" placeholder="">
        </div>
        <div class="form-group">
          <h4 class="font-weight-300">Assistive device and / or bracing</h4>
          <input type="text" name="aid" id="aid" class="form-control form-flatline-415" placeholder="">
        </div>
      </form>
    </div>
  </div>
  <div class="card card-shadow card-highlight">
    <span class="pull-right"><button type="button" class="btn opie-secondary-btn" data-toggle="modal" data-target="#mySST1">
      RECORD
    </button></span>
    <div class="header">
      <h4 class="font-weight-300">Self-Selected Velocity <span class="secondary-opie-color">Trial 1</span></h4>
    </div>
    <h6><span class="opie-color trial-one-result"></span></h6>

    <!-- Button trigger modal -->


    <!-- Modal -->
    <div class="modal fade" id="mySST1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="myModalLabel">Self-Selected Velocity
              <span class="secondary-opie-color">Trial 1</span></h4>
          </div>
          <div class="modal-body text-center">
            <div class="basic stopwatch" id="trial-one"></div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn opie-secondary-btn" data-dismiss="modal">CANCEL</button>
            <button type="button" class="btn opie-secondary-btn" onClick="saveTrialOne()">SAVE</button>
          </div>
        </div>
      </div>
    </div>
  </div>


  <!-- begin: trial two -->
