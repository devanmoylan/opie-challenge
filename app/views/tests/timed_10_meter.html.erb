  <div class="card card-shadow" style="margin-top: 15px;">
    <div class="header">
      <span class="pull-right">
        <button class="btn btn-success shadow-btn" type="button" data-toggle="collapse" data-target="#collapseResults"
                aria-expanded="false" aria-controls="collapseResults" onClick="averageFastVelocityTime();
            averageSelectedVelocityTime(); saveName(); saveDate(); saveAid()">FINALIZE</button>
        </span>
      <h3 class="font-weight-300">Report</h3>
      <div class="collapse" id="collapseResults">
        <div class="well" style="margin-top: 21px;">
          <h4 id="output-name"></h4>
          <h4 id="output-date"></h4>
          <h4 id="output-aid"></h4>
          <h6 class="trial-one-result"></h6>
          <h6 class="trial-two-result"></h6>
          <h6 class="trial-three-result"></h6>
          <h6 class="selected-velocity-time-average"></h6>
